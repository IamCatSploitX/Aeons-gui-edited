   if k == key then
       enabled = not enabled
   end
end)

function Autoblock()
game.ReplicatedStorage.Events.WandEvent:FireServer(3)
wait(2)
end

spawn(function()
   while wait() do
       if enabled then
           repeat
           repeat
		   repeat
		   repeat
		   repeat
		   repeat
		   repeat
		   repeat
               Autoblock()
           until enabled == false
           until enabled == false
		   until enabled == false
		   until enabled == false
		   until enabled == false
		   until enabled == false
		   until enabled == false
		   until enabled == false
       end
   end
end)
end)

