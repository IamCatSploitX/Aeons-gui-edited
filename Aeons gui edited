        local v = (mouse.Hit.p).unit * Vector3.new(math.random(-30, 30),-50,math.random(-30, 30))
        local w = v * 100
        local x = math.random(100, 1000)
        local y = mouse.Hit.p + -v + Vector3.new(math.random(-20, 20), math.random(200, 2000), math.random(-20, 20)) + -v * math.random(0, 0)
